                                 		nedg@0:;***end edge on encoder 2***
                                 
                                 		cpi MS_DIFL, @1
                                 		brlo ndnd@0
                                 			clear_led
                                 			rjmp dndn@0
                                 		ndnd@0:
                                 			set_led
