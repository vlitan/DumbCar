                                 		.endif
                                 			set_led
                                 		rjmp all_fine@0
                                 		ds_lower@0:;higher speed
                                 		.if @0 == 1
                                 			dec VL
                                 		.else
                                 			dec VR
                                 		.endif
                                 		all_fine@0:
