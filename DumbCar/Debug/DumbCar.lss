                                 .def MAP_RES=r0
                                 .def MS_DIFL=r18
                                 .def MS_DIFH=r19
                                 .def LAST_MS1L=r5
                                 .def LAST_MS1H=r6
                                 .def LAST_MS2L=r7
                                 .def LAST_MS2H=r8
                                 
                                 .macro init_vars ;clear registers
                                 	clr MS_DIFH
                                 	clr MS_DIFL
